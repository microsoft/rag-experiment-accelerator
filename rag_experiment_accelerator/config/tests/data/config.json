{
    "experiment_name": "experiment_1",
    "job_name": "baseline",
    "job_description": "",
    "data_formats": ["test_data_format"],
    "main_instruction": "",
    "use_checkpoints": true,
    "sampling": {
        "sample_data": false,
        "percentage": 5,
        "optimum_k": "auto",
        "min_cluster": 2,
        "max_cluster": 30
    },
    "index": {
        "index_name_prefix": "test_prefix",
        "ef_construction": [300],
        "ef_search": [400],
        "chunking": {
            "preprocess": false,
            "chunk_size": [512],
            "overlap_size": [128],
            "generate_title": false,
            "generate_summary": false,
            "override_content_with_summary": false,
            "chunking_strategy": "basic",
            "azure_document_intelligence_model": "prebuilt-read"
        },
        "embedding_model": [
            {
                "type": "sentence-transformer",
                "model_name": "all-MiniLM-L6-v2"
            },
            {
                "type": "azure",
                "model_name": "text-embedding-ada-002"
            }
        ]
    },
    "language":{
        "analyzer": {
            "analyzer_name": "en.microsoft",
            "index_analyzer_name": "",
            "search_analyzer_name": "",
            "char_filters": [],
            "tokenizers": [],
            "token_filters": []
        },
        "query_language": "en-us"
    },
    "rerank": {
        "enabled": true,
        "type": "test_rerank_type",
        "llm_rerank_threshold": 6,
        "cross_encoder_at_k": 7,
        "cross_encoder_model": "test_crossencoder_model"
    },
    "search": {
        "retrieve_num_of_documents": 9,
        "search_type": [
            "test_search_type_1",
            "test_search_type_2"
        ],
        "search_relevancy_threshold": 0.11
    },
    "query_expansion": {
        "query_expansion": false,
        "hyde": "disabled",
        "min_query_expansion_related_question_similarity_score": 90,
        "expand_to_multiple_questions": false
    },
    "openai": {
        "azure_oai_chat_deployment_name": "test_chat_deployment_name",
        "azure_oai_eval_deployment_name": "test_eval_deployment_name",
        "temperature": 10
    },
    "eval": {
        "metric_types": [
            "test_metric_type_1",
            "test_metric_type_2"
        ],
        "eval_data_jsonl_file_path": "./artifacts/eval_data.jsonl"
    }
}